cts-list\">\r\n            <li v-for=\"item in products\" :key=\"item.id\" class=\"product-item\">\r\n                <b><span class=\"product-details\">{{ item.name }} - ₱{{ item.price }}</span></b>\r\n                <p></p>\r\n                <button @click=\"addToCart(item)\" class=\"add-to-cart-btn\">Add to Cart</button>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { cart } from '../main.js';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            products: [\r\n                { name: 'Tapsilog', price: 105 },\r\n                { name: 'Longsilog', price: 75 },\r\n                { name: 'Tocilog', price: 95 },\r\n                { name: 'Hotsilog', price: 75 },\r\n                { name: 'Bangsilog', price: 85 },\r\n                { name: 'Chicksilog', price: 85 },\r\n                { name: 'Bacsilog', price: 75 },\r\n                { name: 'Spamsilog', price: 65 },\r\n                { name: 'Cornsilog', price: 65 },\r\n                { name: 'Tunasilog', price: 75 },\r\n            ],\r\n        };\r\n    },\r\n    methods: {\r\n        addToCart(item) {\r\n            const existingProductIndex = cart.findIndex(product => product.name === item.name);\r\n            if (existingProductIndex !== -1) {\r\n                \r\n                window.alert(`${item.name} is already exists in your cart!`);\r\n                return;\r\n            }\r\n\r\n            // If the product is not in the cart, add it\r\n\r\n            cart.push({\r\n                name: item.name,\r\n                price: item.price,\r\n                unitPrice: item.price,\r\n                quantity: 1,\r\n            });\r\n\r\n            window.alert(`Added ${item.name} to your cart!`);\r\n        },\r\n    }\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n.products-heading {\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.products-list {\r\n  display: flex;\r\n  flex-wrap: wrap; /* Wrap items to the next row */\r\n  gap: 50px; /* Spacing between items */\r\n  padding: 0;\r\n}\r\n\r\n.product-item {\r\n  flex: 20; /* Allow items to grow to fill available space */\r\n  min-width: 200px; /* Minimum width of each item */\r\n  max-width: calc(50% - 20px); /* Maximum width (50% minus spacing) for two items per row */\r\n  border: 3px solid #ccc;\r\n  padding: 10px;\r\n}\r\n\r\n.product-details {\r\n  font-size: 20px;\r\n}\r\n\r\n.add-to-cart-btn {\r\n  background-color: #0d7f23;\r\n  color: #fff;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 25px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.add-to-cart-btn:hover {\r\n  background-color: #0d7f23;\r\n}\r\n\r\n</style>\r\n\r\n\r\n"],"mappings":";;EACSA,KAAK,EAAC;AAAS;gCAChBC,mBAAA,CAA+C;EAA3CD,KAAK,EAAC;AAAkB,GAAC,eAAa;;EACtCA,KAAK,EAAC;AAAe;;EAERA,KAAK,EAAC;AAAiB;gCAChCC,mBAAA,CAAO;mBANvB;;uBACIC,mBAAA,CASM,OATNC,UASM,GARFC,UAA+C,EAC/CH,mBAAA,CAMK,MANLI,UAMK,I,kBALDH,mBAAA,CAIKI,SAAA,QARjBC,WAAA,CAI+BC,KAAA,CAAAC,QAAQ,EAAhBC,IAAI;yBAAfR,mBAAA,CAIK;MAJyBS,GAAG,EAAED,IAAI,CAACE,EAAE;MAAEZ,KAAK,EAAC;QAC9CC,mBAAA,CAA+E,YAA5EA,mBAAA,CAAwE,QAAxEY,UAAwE,EAAAC,gBAAA,CAAvCJ,IAAI,CAACK,IAAI,IAAG,MAAI,GAAAD,gBAAA,CAAGJ,IAAI,CAACM,KAAK,iB,GACjEC,UAAO,EACPhB,mBAAA,CAA6E;MAApEiB,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,SAAS,CAACX,IAAI;MAAGV,KAAK,EAAC;OAAkB,aAAW,iBAPpFsB,UAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}